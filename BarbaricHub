UIGradient_5.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(18, 219, 171)), ColorSequenceKeypoint.new(0.49, Color3.fromRGB(69, 218, 111)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(158, 216, 89))}
UIGradient_5.Parent = Phenomhub

local UIS = game:GetService("UserInputService")

UIS.InputBegan:Connect(function(key)
	if key.KeyCode == Enum.KeyCode.LeftControl and Frame.Visible == true then
		Frame.Visible = false
	else
		Frame.Visible = true
	end
end)
